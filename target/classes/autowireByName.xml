<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="byname"
		class="com.spring.project.autowire_byName.ByName" autowire="byName">
		<property name="roll" value="0009"></property>
		<property name="name" value="Shree"></property>
	</bean>

	<bean name="adBN"
		class="com.spring.project.autowire_byName.AddressByName">
		<property name="pin" value="1234"></property>
		<property name="name" value="Pune"></property>
	</bean>

	<bean name="addN"
		class="com.spring.project.autowire_byName.AddressByName">
		<property name="pin" value="1234"></property>
		<property name="name" value="Pune"></property>
	</bean>










</beans>